JanelaGerenciadora:
    TelaDePerfis:
    PerfisdeExtracao:
    InformacoesDePerfis:
    AcompanharExtracao:

<TelaDePerfis>
    name: 'direcao'
    MDScreen:
        BoxLayout:
            orientation: 'vertical'
            pos_hint: {'x': 0.4,'y': 0.5}
            MDFlatButton:
                text: "EMPRESA"
                theme_text_color: "Custom"
                text_color: 0, 0, 1, 1
                on_release: app.root.current = 'perfis'
            MDFlatButton:
                text: "LABORATORIO"
                theme_text_color: "Custom"
                text_color: 0, 0, 1, 1
                on_release: app.root.current = 'perfis'

<PerfisdeExtracao>:
    name: 'perfis'
    MDScreen:

        BoxLayout:
            orientation: 'vertical'
            MDIconButton:
                icon: "alpha-i-circle"
                theme_text_color: "Custom"
                text_color: app.theme_cls.primary_color 
                pos_hint: {'x': .9}
                on_release: app.root.current = 'informacoes'
            MDFillRoundFlatButton:
                text: "PERFIS DE EXTRACAO"
                size_hint_x: 0.6
                center_x: 0.5
                pos_hint: {'center_x': 0.5,'center_y': 0.9}
            MDBoxLayout:
                spacing: 40
                padding: 100
                MDCard:
                    radius: 30
                    MDBoxLayout:
                        size_hint: 1,1
                        orientation: 'vertical'
                        MDIcon:
                            icon: "account-circle" 
                            theme_text_color: "Custom"
                            text_color: app.theme_cls.primary_color             
                            font_size: "150sp"
                            pos_hint: {'center_x': 0.55,'center_y': 0.9}
                        MDLabel:
                            text: "PERFIL 1"
                            pos_hint: {'center_x': 0.80, 'y': 1}
                MDCard:
                    radius: 30
                    MDBoxLayout:
                        size_hint: 1,1
                        orientation: 'vertical'
                        MDIcon:
                            icon: "account-circle"
                            theme_text_color: "Custom"
                            text_color: app.theme_cls.primary_color
                            font_size: "150sp"
                            pos_hint: {'center_x': 0.55,'center_y': 0.9}
                        MDLabel:
                            text: "PERFIL 2"
                            pos_hint: {'center_x': 0.80, 'y': 1}
                MDCard:
                    radius: 30
                    MDBoxLayout:
                        size_hint: 1,1
                        orientation: 'vertical'
                        MDIcon:
                            icon: "account-circle"
                            theme_text_color: "Custom"
                            text_color: app.theme_cls.primary_color
                            font_size: "150sp"
                            pos_hint: {'center_x': 0.55,'center_y': 0.9}
                        MDLabel:
                            text: "PERFIL 3"
                            pos_hint: {'center_x': 0.80, 'y': 1}
                                           
